{
  "type": "module",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.4.0",
    "@types/node": "^24.8.1",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^61.1.4",
    "eslint-plugin-unicorn": "^61.0.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "tsr": "^1.3.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "format:check": "prettier . --cache --check --ignore-path .gitignore",
    "lint:check": "eslint **/*.{js,ts} --concurrency auto --cache --cache-location node_modules/.cache/.eslintcache",
    "type:check": "tsc --noEmit",
    "unused": "tsr 'src/index.ts$' -p tsconfig.build.json",
    "checkall": "npm run unused && npm run format:check && npm run lint:check && npm run type:check",
    "fix": "npm run unused -- -w; prettier . --cache --write --log-level warn --ignore-path .gitignore; npm run lint:check -- --fix;",
    "test": "vitest run",
    "test:open": "vitest --ui",
    "build": "rimraf dist && tsc -p tsconfig.build.json && tsc-alias -f -p tsconfig.build.json"
  }
}
